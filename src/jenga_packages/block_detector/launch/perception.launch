<launch>
  <!-- Set the path to your rosbag file -->
  <!-- <arg name="bagfile" default="$(find perception)/block_detector/data/franka.bag"/> -->

  <!-- Launch the rosbag node -->
    <!-- <node name="rosbag" pkg="rosbag" type="play" args="$(arg bagfile)">
    </node> -->

  <group ns="ransac">
      <rosparam file="$(find block_detector)/config/ransac_params.yaml" />
  </group>

  <!-- Launch the rec_data node -->
    <!-- <node name="rec_data" pkg="block_detector" type="rec_data_main.py" output="screen"> -->
    <!-- </node> -->
  <!-- read static tf params from config -->
  <rosparam file="$(find block_detector)/config/azura_calib.yaml" command="load" />
  <node pkg="tf" type="static_transform_publisher" name="my_static_tf_publisher" 
    args="$(arg x) $(arg y) $(arg z) $(arg qx) $(arg qy) $(arg qz) $(arg qw) 
            /panda_link0 /camera_base">
    <param name="x" value="$(arg pose.position.x)"/>
    <param name="y" value="$(arg pose.position.y)"/>
    <param name="y" value="$(arg pose.position.y)"/>
    <param name="z" value="$(arg pose.position.z)"/>
    <param name="qx" value="$(arg pose.orientation.x)"/>
    <param name="qy" value="$(arg pose.orientation.y)"/>
    <param name="qz" value="$(arg pose.orientation.z)"/>
    <param name="qw" value="$(arg pose.orientation.w)"/>
  </node>
    
</launch>